<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruyuan.eshop.market.mapper.FreightTemplateMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ruyuan.eshop.market.domain.entity.FreightTemplateDO">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="region_id" property="regionId"/>
        <result column="shipping_amount" property="shippingAmount"/>
        <result column="condition_amount" property="conditionAmount"/>
        <result column="gmt_create" property="gmtCreate"/>
        <result column="gmt_modified" property="gmtModified"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        , name, region_id, shipping_amount, condition_amount, gmt_create, gmt_modified
    </sql>

</mapper>
